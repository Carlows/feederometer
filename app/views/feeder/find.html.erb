<div class="container">
	<% if defined? @model %>

		<h3>Summoner name: <%=@model[:name]%></h3>
		<img src="http://ddragon.leagueoflegends.com/cdn/5.24.2/img/profileicon/<%= @model[:icon_id] %>.png" />
		<h2>Feed percentage <%= Integer(@model[:feeder_percentage]) %>%
	<% else %>
		<h3><%= @message %></h3>
	<% end %>
</div>